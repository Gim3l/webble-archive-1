{
  "name": "simpleshop",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz db studio",
    "build": "blitz build",
    "migrate": "blitz db migrate",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "postinstall": "prisma generate && blitz db migrate",
    "prisma:push": "prisma db push --preview-feature"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "5.13.x",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@opuscapita/react-filemanager": "1.1.2",
    "@opuscapita/react-filemanager-connector-node-v1": "1.1.2",
    "@prisma/cli": "2.x",
    "@prisma/client": "2.x",
    "@types/minio": "7.0.6",
    "blitz": "0.25.0-canary.1",
    "bumbag": "1.3.4",
    "bumbag-server": "1.1.16",
    "chonky": "1.1.2",
    "dayjs": "1.9.4",
    "filepond": "4.20.1",
    "formidable": "1.2.2",
    "formik": "2.x",
    "minio": "7.0.16",
    "multiparty": "4.2.2",
    "passport-google-oauth20": "2.0.0",
    "react": "0.0.0-experimental-7f28234f8",
    "react-color": "2.18.1",
    "react-content-loader": "5.1.4",
    "react-datepicker": "3.3.0",
    "react-doc-viewer": "0.1.5",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "react-error-boundary": "2.3.2",
    "react-filepond": "7.1.0",
    "secure-password": "4.0.0",
    "zod": "1.x",
    "zustand": "3.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/react-hooks": "3.4.1",
    "@types/jest": "26.0.14",
    "@types/react": "16.9.49",
    "@types/secure-password": "3.1.0",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "babel-eslint": "10.1.0",
    "eslint": "7.9.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.x",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "react-test-renderer": "16.13.1",
    "ts-jest": "26.4.0",
    "typescript": "4.x"
  },
  "private": true
}
